#include<stdio.h>
int input()
{
    int a;
    printf("enter the 1st number:\n");
    scanf("%d",&a);
    return a;
}
 
int compute(int a,int b)
{
    int n;
    n=a+b;
    return n;
}
void output(int n)
{
     printf("the number is=%d\n",n);
}
int main()
{
     int a,b,n;
     a=input();
     b=input();
     n=compute(a,b);
     output(n);
}


